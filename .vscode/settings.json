{
	"haxe.displayConfigurations": [
		["display.hxml"]
	],
	"[haxe]": {
		"editor.formatOnSave":true
	},
	"haxecheckstyle.sourceFolders": [
		"src",
		"schema",
		"test"
	],
	"haxeTestExplorer.testCommand": [
